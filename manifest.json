{
  "name": "TabKommander",
  "version": "0.1",
  "description": "Helping Nilson get a grip...",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "List those tabs"
  },
  "permissions": [
    "tabs", "activeTab"
  ],
  "manifest_version": 2
}

